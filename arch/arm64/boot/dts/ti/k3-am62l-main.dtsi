			interrupts = <GIC_SPI 215 IRQ_TYPE_LEVEL_HIGH>, /* irq.0 */
				     <GIC_SPI 215 IRQ_TYPE_LEVEL_HIGH>; /* irq.0 */
			interrupt-names = "host", "peripheral";
			maximum-speed = "high-speed";
			dr_mode = "otg";
			bootph-all;
			snps,usb2-gadget-lpm-disable;
			snps,usb2-lpm-disable;
		};
	};

