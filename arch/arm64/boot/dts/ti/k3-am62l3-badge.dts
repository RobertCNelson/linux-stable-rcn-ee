	mcp23s18: gpio@0 {
		compatible = "microchip,mcp23s18";
		pinctrl-names = "default";
		pinctrl-0 = <&mcp23s18_reset_pins_default>;
		reg = <0>;
		spi-max-frequency = <10000000>;
		gpio-controller;
		#gpio-cells = <2>;
		microchip,spi-present-mask = <0x01>;
		reset-gpios = <&main_gpio0 25 GPIO_ACTIVE_LOW>;
	};
};

